{
  "name": "@edno/docusaurus2-graphql-doc-generator",
  "homepage": "https://graphql-markdown.github.io",
  "description": "Docusaurus 2 plugin for generating Markdown documentation from a GraphQL schema.",
  "bugs": {
    "url": "https://github.com/graphql-markdown/graphql-markdown/issues"
  },
  "version": "1.11.0",
  "license": "MIT",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-markdown/graphql-markdown.git"
  },
  "keywords": [
    "docusaurus",
    "graphql",
    "graphql-schema",
    "documentation",
    "plugin",
    "markdown",
    "graphql-markdown"
  ],
  "author": {
    "name": "Gregory Heitz"
  },
  "contributors": [
    {
      "name": "jocrau"
    },
    {
      "name": "mimo84"
    },
    {
      "name": "coder2034"
    },
    {
      "name": "lauzierj"
    },
    {
      "name": "jineshshah36"
    },
    {
      "name": "GuiHash"
    },
    {
      "name": "davidyaha"
    },
    {
      "name": "jlndk"
    },
    {
      "name": "elias-pap"
    }
  ],
  "scripts": {
    "prettier": "prettier \"{src,tests,assets}/**/*.{js,json,md}\"",
    "lint": "eslint \"{src,tests,assets}/**/*.js\"",
    "test": "jest",
    "test:unit": "jest --projects tests/unit",
    "test:integration": "jest --projects tests/integration",
    "test:smoke": "earthly +smoke-test",
    "test:coverage": "sh ./scripts/coverage.sh",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "@graphql-inspector/core": "^3.1.2",
    "@graphql-tools/graphql-file-loader": "^7.3.14",
    "@graphql-tools/load": "^7.5.13"
  },
  "peerDependencies": {
    "graphql": "^14.0 || ^15.0 || ^16.0",
    "prettier": "^2.5"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "devDependencies": {
    "@stryker-mutator/core": "6.1.2",
    "@stryker-mutator/jest-runner": "6.1.2",
    "commitizen": "4.2.5",
    "cz-emoji": "1.3.1",
    "diff": "5.1.0",
    "directory-tree": "3.3.0",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "graphql": "16.5.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-file-snapshot": "0.5.0",
    "lodash.filter": "4.6.0",
    "mock-fs": "5.1.4",
    "pinst": "3.0.0",
    "prettier": "2.7.1"
  },
  "prettier": {
    "singleQuote": false,
    "bracketSpacing": true,
    "proseWrap": "preserve",
    "overrides": [
      {
        "files": "*.js",
        "options": {
          "arrowParens": "always",
          "printWidth": 80,
          "trailingComma": "all",
          "tabWidth": 2
        }
      }
    ]
  },
  "directories": {
    "test": "tests"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=14.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    },
    "cz-emoji": {
      "skipQuestions": [
        "scope",
        "issues",
        "breaking"
      ],
      "types": [
        {
          "emoji": "✨",
          "code": ":sparkles:",
          "description": "Add a new feature.",
          "name": "feat"
        },
        {
          "emoji": "🐛",
          "code": ":bug:",
          "description": "Fix a bug.",
          "name": "fix"
        },
        {
          "emoji": "📦️",
          "code": ":package:",
          "description": "Update dependencies.",
          "name": "build"
        },
        {
          "emoji": "🔧",
          "code": ":wrench:",
          "description": "Update configuration files.",
          "name": "conf"
        },
        {
          "emoji": "♻️",
          "code": ":recycle:",
          "description": "Refactor code.",
          "name": "refactor"
        },
        {
          "emoji": "🧪",
          "code": ":test_tube:",
          "description": "Update tests.",
          "name": "test"
        },
        {
          "emoji": "👷",
          "code": ":construction_worker:",
          "description": "Update CI.",
          "name": "ci"
        },
        {
          "emoji": "📝",
          "code": ":memo:",
          "description": "Update documentation.",
          "name": "docs"
        },
        {
          "emoji": "🔖",
          "code": ":bookmark:",
          "description": "Add a tag / release.",
          "name": "tag"
        },
        {
          "emoji": "🧑‍💻",
          "code": ":technologist:",
          "description": "Other changes.",
          "name": "other"
        }
      ]
    }
  }
}
