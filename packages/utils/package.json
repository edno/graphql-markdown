{
  "name": "@graphql-markdown/utils",
  "homepage": "https://graphql-markdown.github.io",
  "description": "GraphQL-Markdown common libraries.",
  "bugs": {
    "url": "https://github.com/graphql-markdown/graphql-markdown/issues"
  },
  "version": "1.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-markdown/graphql-markdown.git"
  },
  "author": {
    "name": "Gregory Heitz"
  },
  "type": "commonjs",
  "exports": {
    ".": "./dist/index.js",
    "./array": "./dist/array.js",
    "./fs": "./dist/fs.js",
    "./graphql": "./dist/graphql.js",
    "./object": "./dist/object.js",
    "./prettier": "./dist/prettier.js",
    "./string": "./dist/string.js",
    "./url": "./dist/url.js"
  },
  "typesVersions": {
    "*": {
      "array": ["dist/array.d.ts"],
      "fs": ["dist/fs.d.ts"],
      "graphql": ["dist/graphql.ts"],
      "object": ["dist/object.ts"],
      "prettier": ["dist/prettier.d.ts"],
      "string": ["dist/string.d.ts"],
      "url": ["dist/url.d.ts"]
    }
  },
  "scripts": {
    "build": "tsc --build --clean && tsc",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "test": "tap",
    "snap": "tap",
    "stryker": "stryker run"
  },
  "dependencies": {
    "@graphql-tools/load": "^7.8.4"
  },
  "peerDependencies": {
    "graphql": "^14.0 || ^15.0 || ^16.0"
  },
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=16.14"
  },
  "publishConfig": {
    "access": "public"
  }
}
