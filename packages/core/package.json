{
  "name": "@graphql-markdown/core",
  "homepage": "https://graphql-markdown.github.io",
  "description": "GraphQL-Markdown core package for generating Markdown documentation from a GraphQL schema.",
  "bugs": {
    "url": "https://github.com/graphql-markdown/graphql-markdown/issues"
  },
  "version": "1.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-markdown/graphql-markdown.git"
  },
  "keywords": [
    "graphql",
    "graphql-schema",
    "documentation",
    "markdown",
    "graphql-markdown"
  ],
  "author": {
    "name": "Gregory Heitz"
  },
  "type": "commonjs",
  "exports": {
    "./config": "./dist/src/config.js",
    "./generator": "./dist/src/generator.js",
    "./groupInfo": "./dist/src/groupInfo.js",
    "./renderer": "./dist/src/renderer.js",
    "./type": "./dist/src/type.js"
  },
  "typesVersions": {
    "*": {
      "config": ["dist/src/config.d.ts"],
      "generator": ["dist/src/generator.d.ts"],
      "groupInfo": ["dist/src/groupInfo.d.ts"],
      "renderer": ["dist/src/renderer.d.ts"],
      "type": ["dist/src/type.d.ts"]
    }
  },
  "scripts": {
    "build": "rm -rf dist/* && tsc -p tsconfig.json",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "test": "tap",
    "snap": "tap",
    "stryker": "stryker run"
  },
  "dependencies": {
    "@graphql-markdown/utils": "^1.1.0"
  },
  "peerDependencies": {
    "@graphql-markdown/printer-legacy": "^1.1.0",
    "@graphql-markdown/diff": "^1.1.0",
    "prettier": "^2.8"
  },
  "peerDependenciesMeta": {
    "@graphql-markdown/printer-legacy": {
      "optional": true
    },
    "@graphql-markdown/diff": {
      "optional": true
    },
    "prettier": {
      "optional": true
    }
  },
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=16.14"
  },
  "publishConfig": {
    "access": "public"
  }
}
